<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:start="0dp"
        android:end="4dp"
        android:bottom="4dp"
        android:top="4dp">
        <shape android:shape="rectangle">
            <corners android:radius="40dp" />
            <size
                android:width="25dp"
                android:height="25dp" />
            <solid android:color="@color/elevated_dark_background" />
        </shape>
    </item>
    <item
        android:start="10dp"
        android:bottom="4dp"
        android:top="4dp">
        <shape android:shape="rectangle">
            <size
                android:width="0dp"
                android:height="25dp" />
            <solid android:color="@color/elevated_dark_background" />
        </shape>
    </item>
</layer-list>